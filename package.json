    "mocha": "^4.0.1",
    "prettier": "^1.14.3",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.12.2",
    "webpack-cli": "^3.0.8",
    "yargs": "^10.0.3"
  },
  "dependencies": {
    "bignumber.js": "^7.2.1",
    "crypto": "^1.0.1",
    "js-sha3": "^0.8.0",
