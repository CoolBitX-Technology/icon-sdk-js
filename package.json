    "test:watch": "NODE_PATH=./ mocha --require babel-register --colors -w ./test/**/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/icon-project/icon-sdk-js/"
  },
  "keywords": [
